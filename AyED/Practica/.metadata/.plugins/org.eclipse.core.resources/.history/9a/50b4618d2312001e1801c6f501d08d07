package Practica6;

public class nodoColor {
	
	private String dato;
	
	private String color;
	
	public nodoColor(String dato, String color){
		this.color = color;
		this.dato = dato;
	}
	
	public setDato(String dato){
		this.dato = dato;
	}
	
	public setColor(color){
		this.color = color;
	}
}
