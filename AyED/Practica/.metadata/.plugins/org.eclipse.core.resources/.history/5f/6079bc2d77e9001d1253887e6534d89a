package Parciales;

import tp02.ejercicio1.*;
import tp02.ejercicio2.ListaEnlazadaGenerica;

public class ListaParcial {
	private ListaDeEnterosEnlazada lista;
	private int pares = 0;
	
	public ListaDeEnterosEnlazada getLista(){
		return lista;
	}
	
	public int getPares(){
		return pares;
	}
	
	public void setPares(int pares){
		this.pares = pares;
	}
	
	public void setLista(ListaDeEnterosEnlazada lista){
		this.lista = lista;
	}
	
	public String toString(){
		String str = "";
		lista.comenzar();
		while(!lista.fin()){
			str += lista.proximo() + " => ";
		}
		str += "\n Pares: " + pares;
		return str;
	}
}
