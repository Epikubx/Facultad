package Practica3;
import tp03.ejercicio1.*;

public class Ej2
{
	public static void main(String[] args)
	{
		ArbolBinario<Integer> arbol = new ArbolBinario<Integer>(3);
		arbol.agregarHijoIzquierdo(crearNodo(5));
		System.out.println(arbol.contarHojas());
	}
	
	private ArbolBinario<Integer> crearNodo(Integer dato){
		ArbolBinario<Integer> arbol = new ArbolBinario<Integer>(dato);
		return arbol;
	}
}