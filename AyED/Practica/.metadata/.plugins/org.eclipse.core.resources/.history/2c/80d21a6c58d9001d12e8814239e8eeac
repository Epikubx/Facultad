package Practica3;

import tp03.ejercicio1.*;

public class RedBinariaLlena {

	private ArbolBinario<Integer> arbol;
	
	public RedBinariaLlena(ArbolBinario<Integer> arbol){
		this.arbol = arbol;
	}
	
	public int retardoReenvio(){
		if(arbol.esHoja()){
			return 0;//no cuento las hojas
		}
		return 1;
	}
}