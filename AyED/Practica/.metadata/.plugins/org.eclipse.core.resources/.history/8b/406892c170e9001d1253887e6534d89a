package Parciales;

import tp02.ejercicio1.*;

public class ListaParcial {
	private ListaDeEnterosEnlazada lista;
	private int pares;
	
	public ListaDeEnterosEnlazada getLista(){
		return lista;
	}
	
	public int getPares(){
		return pares;
	}
	
	public void setPares(int dato){
		this.pares = dato;
	}
	
	public void setLista(ListaDeEnterosEnlazada lista){
		this.lista = lista;
	}
}
